@{
    ViewData["title"] = "Register";
    ViewData["temp"] = "testing";
    
}

@section Scripts
{
    <script src="~/js/Register/index.js"></script>
}

<style>
    #err_msg {
        color: red;
        font-weight: bold;
        padding: 10px;
    }
</style>

<p></p>

<form id="form" action="/Register/TEMP" method="post">
    <table align="center">
        <tr>
            <td>
                New Username:
            </td>
            <td>
                <input id="newUsername" name="newUsername"
                       class="form-control" />
            </td>
        </tr>
        <tr>
            <td>
                Password:
            </td>
            <td>
                <input id="newPassword1" name="newPassword1"
                       type="password" class="form-control" />
            </td>
        </tr>
        <tr>
            <td>
                Confirm:
            </td>
            <td>
                <input id="newPassword2" name="newPassword2"
                       type="password" class="form-control" />
            </td>
        </tr>

        <tr>
            <td colspan="2" align="right">
                <input id="submitBtn" type="submit" class="btn btn-success" />
            </td>
        </tr>
        <tr>
            <td colspan="2" align="center">
                <div id="err_msg">@ViewData["errMsg"]</div>
            </td>
        </tr>
    </table>
</form>